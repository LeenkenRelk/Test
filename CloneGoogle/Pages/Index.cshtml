@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<form method="post">
    <input asp-for="Changes_Urls!.OriginUrl" name="Changes_Urls.OriginUrl" />
    <input name="Changes_Urls.ShortUrl" asp-for="Changes_Urls!.ShortUrl" placeholder="Введите короткий URL" required />
    <input type="hidden" name="Changes_Urls.CreateUrlTime" value="@DateTime.Now" />
    <input type="hidden" name="Changes_Urls.ClickUrl" value="0" />
    <input type="submit" />
</form>